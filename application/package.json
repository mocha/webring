{
  "name": "webring",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run prebuild && concurrently \"next dev\" \"chokidar '../directory/**/*.yaml' '../directory/**/*.yml' '../ringlets/**/*.yaml' '../ringlets/**/*.yml' -c 'npm run prebuild'\"",
    "prebuild": "node utils/prebuild.js && node utils/generate-ringlet-files.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "generate-ringlet-files": "node utils/generate-ringlet-files.js"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.477.0",
    "next": "^14.2.24",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.16",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
